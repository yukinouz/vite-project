{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jquery": true
  },
  "extends": ["eslint:recommended"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error",
    "no-const-assign": "warn",
    "no-undef": "warn",
    "no-control-regex": "warn",
    "no-unused-vars": "warn",
    "no-useless-escape": "warn",
    "quotes": "warn"
  },
  "ignorePatterns": ["dist"]
}
